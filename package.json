	"devDependencies": {
		"@types/bn.js": "^5.1.0",
		"@types/chai": "^4.3.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "^18.7.13",
		"chai": "^4.3.4",
		"mocha": "^9.0.3",
		"prettier": "^2.6.2",
		"ts-mocha": "^8.0.0",
		"typescript": "^4.3.5"
